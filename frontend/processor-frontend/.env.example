# Vite environment variables (must start with VITE_)
# Copy this file to .env or .env.local and adjust values.

# Base API URL for backend (no trailing slash)
VITE_API_URL=http://127.0.0.1:8000

# Feature flags
VITE_FEATURE_ENABLE_DARK_MODE=true
VITE_FEATURE_SHOW_BETA=false

# Optional analytics/public keys
VITE_PUBLIC_SENTRY_DSN=
VITE_PUBLIC_POSTHOG_KEY=

# Payment or integration public keys (never put secrets here)
VITE_PUBLIC_PAYSTACK_KEY=

